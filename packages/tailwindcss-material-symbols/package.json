  },
  "peerDependencies": {
    "tailwindcss": "^3.4.17 || ^4.1.13",
    "vite": "^7.1.7"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "subset-font": "^2.4.0"
  },
  "devDependencies": {
    "@types/subset-font": "^2.3.0",
