{
  "name": "@janispritzkau/tailwindcss-material-symbols",
  "version": "0.0.8",
  "description": "Easy-to-use, production-ready Material Symbols integration for Tailwind CSS with built-in font subsetting.",
  "keywords": [
    "material-symbols",
    "tailwindcss",
    "tailwind",
    "plugin",
    "vite",
    "icons",
    "subset-font"
  ],
  "homepage": "https://github.com/janispritzkau/tailwindcss-material-symbols",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janispritzkau/tailwindcss-material-symbols.git",
    "directory": "packages/tailwindcss-material-symbols"
  },
  "bugs": {
    "url": "https://github.com/janispritzkau/tailwindcss-material-symbols/issues"
  },
  "author": "Janis Pritzkau <janispritzkau@gmail.com>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "tailwindcss-material-symbols": "./bin/tailwindcss-material-symbols.js"
  },
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "style": "./dist/index.css",
      "types": "./dist/index.d.ts"
    },
    "./subset": {
      "import": "./dist/subset.js",
      "types": "./dist/subset.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./full.css": "./full.css",
    "./outlined.css": "./outlined.css",
    "./rounded.css": "./rounded.css",
    "./sharp.css": "./sharp.css"
  },
  "files": [
    "assets",
    "dist",
    "full.css",
    "outlined.css",
    "rounded.css",
    "sharp.css"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "peerDependencies": {
    "tailwindcss": "^3.4.17 || ^4.1.13",
    "vite": "^7.1.7"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "subset-font": "^2.4.0"
  },
  "devDependencies": {
    "@types/subset-font": "^2.3.0",
    "cac": "^6.7.14",
    "lightningcss": "^1.30.2",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.7"
  }
}
